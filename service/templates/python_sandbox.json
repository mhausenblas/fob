{
    "acceptedResourceRoles": [
        "slave_public"
    ],
    "cmd": "curl https://gist.githubusercontent.com/mhausenblas/e5aaac01e963bd0761189d1360b79bc4/raw/230f31cff345a79347b2c6c21c52d3987e557e5f/fob_example.py > fobfun.py && python fob_driver.py",
    "container": {
        "docker": {
            "image": "mhausenblas/fob:pydriver",
            "network": "BRIDGE",
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 0
                }
            ]
        },
        "type": "DOCKER"
    },
    "cpus": 0.1,
    "id": "fob-aviary/template",
    "mem": 100
}
